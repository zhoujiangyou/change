/* pages/change/list.wxss */
.main_contain{
  width: 100%;
  height: 100%;
  position: absolute;
  display: block;
}
/* 导航栏背景*/
.top_bar {
	width: 100%;
	height: 6%;
  position:fixed;
	background-color: RGB（255,255,255）
  }
   .top_bar .scroll_view_H{
    width: 85%;
    height: 100%;
    overflow: auto;
    white-space:nowrap;
  }
  .top_bar .top_bar_all{
   width: 15%;
   display: flex;
   align-items: center;
   justify-content: center;
  }
  .top_bar .top_bar_allimg{
    width: 75%;
    height: 100%; 
    background-image: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAB4AAAAeCAYAAAA7MK6iAAAAlklEQVRIie2Wyw3DIBAFB6pwUgod2GUkKYpL6khDyDhV4IN98IXPSpYPZp/EaYc3QkIIk7wbgS/woJwIvIFfZi7qMcm7CAwV+LjpWZg191gBDOXTiHqsAD41KlbxPcV/Ab8UZpKeaIEXEBrgwPZk5iLp+ZjkXQN7fvq8XCq+TDwBM5Aqa97ZXEQ9+gO5LCpW8T3Fff1AVn0lO37YiZe8AAAAAElFTkSuQmCC');
    background-repeat: no-repeat;
    background-size: 65%;
    background-position: center center;
  } 
.top_bar_flex{
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;

}
/*导航栏单个菜单 scroll-view 不支持flex*/
.scroll_view_item{
  display: inline-flex;
  padding: 0 5%;
  height: 100%;
  align-items: center;
}
/*导航栏菜单字体样式*/
.scroll_view_item_style{
  font-family: PingFang-SC-Medium;
	font-size: 15px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: rgba(0, 0, 0, 0.5);
}

.top_and_contain{
  width: 750px;
	height: 1px;
  background-color:rgb(238,240,242);
  position: fixed;
  top: 6%;
}
.contain{
  width: 100%;
  height: 80%;
  top:7%;
  position: fixed;
}
.contain .change_list{
  width: 90%;
  height: 100%;
  margin:0 auto;         
  padding-top: 1%;
}
.contain .change_list .change_list_item{
  width: 100%;
  height: 60%;
  padding-bottom: 10px;
}
.contain .change_list .change_list_item .cli_hang1{
  width: 100%; 
  height: 15%;
  display: flex;
  flex-direction: row;
  align-items: center;
}

/*列表头像  */
.contain .change_list .change_list_item .cli_hang1 .cli_avatar{
  background-color: yellowgreen;
  border:2px solid #000;
  border-radius: 100px;
  width: 30px;
  height: 30px;
  margin-left: 5px;
}
/*列表姓名  */
.contain .change_list .change_list_item .cli_hang1 .cli_name{
	font-family: PingFang-SC-Bold;
	font-size: 17px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #404a54;
  padding-left: 5%;
}
/*列表时间  */
.contain .change_list .change_list_item .cli_hang1 .cli_time{
	font-family: PingFang-SC-Medium;
	font-size: 12px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #98a4ab;
  padding-left: 51%;
}
/*第二行  */
.contain .change_list .change_list_item .cli_hang2{
  width: 100%; 
  height:50%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.contain .change_list .change_list_item .cli_hang2 .cli_img{
  height: 100%;
  width: 49%;
}
/*第三行  */
.contain .change_list .change_list_item .cli_hang3{
  width: 100%;
  height: 8%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.contain .change_list .change_list_item .cli_hang3 view{
	font-family: PingFang-SC-Medium;
	font-size: 12px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #98a4ab;
}

.contain .change_list .change_list_item .cli_hang4{
  width: 100%;
  height:15%;
}
.contain .change_list .change_list_item .cli_hang4 text{
  width: 100%;
  height: 100%;
	font-family: PingFang-SC-Medium;
	font-size: 15px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #565d61;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden
}

.contain .change_list .change_list_item .cli_hang5{
  width: 100%;
  height:12%;
  display: flex;
  justify-content: space-between;
}
.cli_more{
  width: 10%;
  height: 100%;
}
.cli_zan_share{
  display: flex;
  width: 32%;
  align-items: center;
}
.cli_zan_share view{
  width: 25%;
  height: 100%;
  padding-right: 5px;
  background-repeat: no-repeat;
  background-size: 50%;
  background-position:center center;
}
.cli_zan_share .image_zan{
  background-image: url('data:image/png;base64,');
}
.cli_zan_share .image_share{
   background-image: url('data:image/png;base64,');
}
.cli_zan_share text{
  width: 15%;
  color: red;
}
::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}

/* 底部导航栏目 */
.bottom_bar{
 width: 100%;
 height: 12%;
 position: fixed;
 bottom: 0;
 display: flex;
 flex-direction: row;
 flex-wrap: nowrap;
 justify-content: space-between;
 	font-family: AdobeHeitiStd-Regular;
	font-size: 10px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #ff913a;
}
.bottom_bar .bottom_bar_item_1{
  flex-grow: 1;
  text-align: center;
}

.bottom_bar1_img{
  width: 50%;
  height: 65%;
  margin: 0 auto;
  background-repeat: no-repeat;
  background-size: 50%;
  background-position: center center;
  background-image: url('data:image/png;base64,');
}

.bottom_bar .bottom_bar_item_2{
  flex-grow: 1;
  text-align: center;
}
.bottom_bar2_img{
  width: 80%;
  height: 72%;
  margin: 0 auto;
  background-repeat: no-repeat;
  background-size:60%;
  background-position: center center;
  background-image: url('data:image/png;base64,');
}
.bottom_bar .bottom_bar_item_3{
  flex-grow: 1;
  text-align: center;
}
.bottom_bar3_img{
  width: 50%;
  height: 65%;
  margin: 0 auto;
  background-repeat: no-repeat;
  background-size: 80%;
  background-position: center center;
  background-image: url('data:image/png;base64,');
}